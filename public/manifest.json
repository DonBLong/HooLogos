{
  "manifest_version": 3,
  "name": "HooLogos",
  "version": "1.0.0",
  "description": "Adds club logos to highlights thumbnails on hoofoot.com",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://*.hoofoot.com/*"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["https://*.hoofoot.com/*"]
    }
  ]
}
